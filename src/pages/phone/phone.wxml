<view class="app phone">
	<view class="app__inner">
		<image src="http://i4.bvimg.com/649796/a3e3c98e90626845.png" class="company_img"></image>
		<view class="company_info">
			<view class="row">
				<text class="label">公司名称:</text>
				<text class="detail">{{companyName}}</text>
			</view>
			<view class="row">
				<text class="label">联系人:</text>
				<text class="detail">{{userName}}</text>
			</view>
			<view class="row" style="margin-bottom: 10px">
				<text class="label">联系电话:</text>
				<text class="detail">{{phone}}</text>
			</view>
			<button type="primary" bindtap="call" size="primary">拨打电话</button>
		</view>
	</view>
</view>
