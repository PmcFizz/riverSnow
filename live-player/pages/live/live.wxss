/* pages/details/detail.wxss */
page{
  background-color: #fff;
  overflow:hidden;
  height:100%;
}
.live-detail{
  background-color: #fff;
  height: 100%;
  width: 100%;
}
.conment-container{
  overflow-y: scroll;
}
.conment-container .comment_item{
  display: flex;
  padding: 0 20rpx;
  font-size:35rpx;
  line-height: 62rpx;
  flex: 1;
  align-items: flex-start
}
.player_box{
  position: relative;
  font-size:0;
}
.player{
  position: relative;
  font-size:0;
  height: 800rpx;
}
.player_box .player_btn{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 70rpx;
  background: rgba(255,255,255,0.2);
  display: flex;
  align-items: center
}

.player_box .player_btn.bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 70rpx;
  background: rgba(255,255,255,0.2);
  display: flex;
  align-items: center
}

.player_box .player_btn .btn_item{
  flex: 1;
  height: 100%;
}
.player_box .player_btn .btn_item .btn{
  width:40rpx;
  height:40rpx;
  margin:15rpx auto;
}
.player_box .player_btn .btn_item.bottom .btn{
  width:40rpx;
  height:40rpx;
  margin:15rpx auto;
  bottom: 0px;
}

.conment-container image{
  width: 46rpx;
  height: 46rpx;
  margin-top:8rpx;
  border-radius: 40%
}
.conment-container .username{
  color: #8fb1df
}
.conment-container text{
  flex: 1;
  word-break: break-all
}
.comment-fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
  z-index: 1000;
  height:50px
}
.comment-fixed.showemoji{
   height:156px
}

.cf-bg {
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: transparent;
  z-index: 99;
}

.cf-box {
  padding: 5px 5px;
  display: flex;
  justify-content: center;
}

.t-info textarea {
  background: red;
  padding: 8px 5px;
  height: 30px;
  box-sizing: border-box;
  width: 100%;
  display: block;
}

.cf-content {
  background-color: #f2f2f2;
  padding: 5px 5px;
  font-size: 12px;
  height: 30px;
  max-height: 40px;
  width: 100%;
  border-radius:5rpx;
}

.cf-send {
  width: 50px;
  display: flex !important;
  flex-direction: column;
  justify-content: center;
  margin-left: 5px;
  border-radius: 2px;
  text-align: center;
  padding: 0 2px !important;
  font-size: 12px;
  background-color: #e64340;
  color: #fff;
}

.cf-send.touch-active:active {
  background-color: #b03f3e;
}

.emoji {
  background: url(data:image/png;base64,) no-repeat center/contain;
  width:30px;
  height:40px;
  text-align:center;
  box-sizing:border-box;
  font-size:20px;
  margin-right: 5px
}

.emoji-box {
  position: relative;
  height: 100px;
  padding: 5px 0;
  box-sizing: border-box;
  margin-bottom: -100px;
}

.emoji-cell {
  width: 9.09%;
  height: 33px;
  display: inline-block;
}

.emoji-cell image {
  width: 23px;
  height: 23px;
  padding: 5px;
  border-radius: 3px;
}

.emoji-move-in {
  -webkit-animation: emoji-move-in 0.3s forwards;
  animation: emoji-move-in 0.3s forwards;
}

.emoji-move-out {
  -webkit-animation: emoji-move-out 0.3s forwards;
  animation: emoji-move-out 0.3s forwards;
}

.no-emoji-move {
  -webkit-animation: none;
  animation: none;
}

@-webkit-keyframes emoji-move-in {
  0% {
    margin-bottom: -200px;
  }

  100% {
    margin-bottom: 0;
  }
}

@keyframes emoji-move-in {
  0% {
    margin-bottom: -200px;
  }

  100% {
    margin-bottom: 0;
  }
}

@-webkit-keyframes emoji-move-out {
  0% {
    margin-bottom: 0;
  }

  100% {
    margin-bottom: -200px;
  }
}

@keyframes emoji-move-out {
  0% {
    margin-bottom: 0;
  }

  100% {
    margin-bottom: -200px;
  }
}

@-webkit-keyframes pd-left-move {
  0% {
    padding-left: 5px;
  }

  100% {
    padding-left: 15px;
  }
}

@keyframes pd-left-move {
  0% {
    padding-left: 5px;
  }

  100% {
    padding-left: 15px;
  }
}
.share_icon{
  width: 50px;
  height:30px;
  padding:0 5px;
  line-height: 30px;
  margin-top: 5px;
  background-color: #34a123;
  color: #fff;
  border-radius: 5px
}
.add_icon{
  background: url(data:image/png;base64,) no-repeat center/contain;
  width: 30px;
  height: 30px
}
.page-body-button {
  margin-bottom: 30rpx;
}

live-player {
  width: 100%;
  height: 300px;
}