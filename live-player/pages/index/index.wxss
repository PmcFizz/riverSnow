.hdzb .title{
  margin: 50rpx 0 20rpx
}
.hdzb .title .text{
  font-size: 50rpx;
  color: #FBC700;
  margin: 0 20rpx
  
}
.hdzb .title .line{
  background: #767676;
  width: 100%;
  height: 1px;
  flex: 1
}
.hdzb .prompt{
  color: #8F8F8F;
  font-size: 24rpx
}
.hdzb .con{
  margin: 80rpx auto
}
.hdzb .con .con_item{
  width:300rpx;
  margin-left:50rpx;
  text-align: center;
  font-size:0;
}
.hdzb .con .con_item image{
  width:300rpx;
  height:300rpx;
  border-radius: 10rpx
}
.hdzb .con .con_item .item_tit{
  font-size: 35rpx;
  color: #FFFFFF;
  margin-top: 20rpx
}
.hdzb .con .con_item .img_box{
  position: relative;
}
.hdzb .con .con_item .type{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  background: url(data:image/png;base64,) no-repeat center/contain;
  width: 57rpx;
  height: 20rpx
}
.hdzb .con .con_item .play{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  background: url(data:image/png;base64,) no-repeat center/contain;
  width: 84rpx;
  height: 84rpx
}
.dialog_box{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0,0,0,0.5)
}
.dialog_box .dialog{
  width: 85%;
  height: 300rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  border-radius: 8rpx;
  background: #fff;
}
.dialog_box .dialog text{
  height:200rpx;
  display:block;
  padding: 50rpx 70rpx 0;
  text-align:  center;
  color: #888;
  line-height: 50rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #ddd;
  font-size: 37rpx
}
.dialog_box .dialog .btn_box button{
  flex: 1;
  background: #fff;
  line-height: 100rpx;
  font-size: 38rpx
}
.dialog_box .dialog .btn_box button:after{
  border: 0 none
}
.dialog_box .dialog .btn_box button.btn2{
  border-left: 1px solid #ddd;
  color: green
}